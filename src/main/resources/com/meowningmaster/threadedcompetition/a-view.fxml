<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="211.0" prefWidth="428.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meowningmaster.threadedcompetition.AController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <Slider fx:id="slider" showTickLabels="true" showTickMarks="true" value="50"/>
   <HBox alignment="CENTER" prefHeight="31.0" prefWidth="388.0">
      <Spinner fx:id="threadIncSpinner">
         <HBox.margin>
            <Insets right="10.0"/>
         </HBox.margin>
      </Spinner>
      <Spinner fx:id="threadDecSpinner">
         <HBox.margin>
            <Insets left="10.0"/>
         </HBox.margin>
      </Spinner>
   </HBox>
   <Button fx:id="startButton" mnemonicParsing="false" text="Start!">
      <font>
         <Font size="16.0"/>
      </font>
      <VBox.margin>
         <Insets top="30.0"/>
      </VBox.margin>
   </Button>
</VBox>
